{
  "name": "nestjs-openapi-validation",
  "description": "OpenAPI validation for NestJS",
  "version": "0.0.3",
  "license": "MIT",
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "main": "./dist/src/lib/index.js",
  "module": "./dist/src/lib/index.js",
  "types": "./dist/src/lib/index.d.ts",
  "scripts": {
    "lib:build": "rimraf dist && tsc --project tsconfig.lib.json && tsc-alias -p tsconfig.lib.json",
    "build": "nest build",
    "dev": "nest start --watch --type-check",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "yarn jest --config ./jest.json",
    "start": "nest start",
    "start:prod": "node dist/main",
    "v:major": "npm version major -m \"chore: bump major to %s\"",
    "v:minor": "npm version minor -m \"chore: bump minor to %s\"",
    "v:patch": "npm version patch -m \"chore: bump patch to %s\""
  },
  "dependencies": {
    "@scalar/nestjs-api-reference": "^0.5.12",
    "radash": "^12.1.1",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.16",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.9",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^30.0.4",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.1.3",
    "vitest": "^3.2.4"
  }
}
